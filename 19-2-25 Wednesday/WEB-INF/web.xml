<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
    <display-name>Techmtraain</display-name>

    <!-- Servlet 1 -->
    <servlet>
        <servlet-name>MyServlet1</servlet-name>
        <servlet-class>mypack.Myservelet1</servlet-class>  <!-- Ensure correct package name -->
    </servlet>

    <servlet-mapping>
        <servlet-name>MyServlet1</servlet-name>
        <url-pattern>/myservlet1</url-pattern>
    </servlet-mapping>

    <!-- Servlet 2 -->
    <servlet>
        <servlet-name>MyFirstServlet</servlet-name>
        <servlet-class>mypack.MyServlet</servlet-class>  <!-- Ensure correct package name -->
    </servlet>

    <servlet-mapping>
        <servlet-name>MyFirstServlet</servlet-name>
        <url-pattern>/myfirstservlet</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
    <welcome-file>index.html</welcome-file>
</welcome-file-list>

<servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>mypack.Login</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>  
</servlet-mapping>


</web-app>